{
  "Name": "Stream Deck Slack Status",
  "Description": "This plugin updates the slack status.",
  "Version": "1.0.0",
  "Author": "ellreka",
  "Icon": "images/icon",
  "Category": "Slack Status",
  "CategoryIcon": "images/categoryIcon",
  "CodePath": "code.html",
  "Actions": [
    {
      "Name": "Update Slack Status",
      "UUID": "net.ellreka.slack-status.update-status",
      "Icon": "images/actionIcon",
      "States": [
        {
          "Image": "images/icon",
          "TitleAlignment": "bottom",
          "FontSize": "12"
        }
      ]
    },
    {
      "Name": "Clear Slack Status",
      "UUID": "net.ellreka.slack-status.clear-status",
      "Icon": "images/actionIcon",
      "States": [
        {
          "Image": "images/icon",
          "TitleAlignment": "bottom",
          "FontSize": "12",
          "Title": "Clear"
        }
      ]
    }
  ],
  "PropertyInspectorPath": "pi.html",
  "URL": "https://github.com/ellreka/streamdeck-slack-status",
  "SDKVersion": 2,
  "OS": [
    {
      "Platform": "mac",
      "MinimumVersion": "10.11"
    },
    {
      "Platform": "windows",
      "MinimumVersion": "10"
    }
  ],
  "Software": {
    "MinimumVersion": "5.0"
  }
}
